{
  "name": "github-contribute-summary",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "encrypt-token": "node scripts/encrypt-token.cjs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage:summary": "node scripts/coverage-summary.cjs",
    "coverage:check": "node scripts/coverage-summary.cjs 80",
    "depcruise": "dependency-cruiser src --config .dependency-cruiser.js",
    "depcruise:graph": "dependency-cruiser src --include-only \"^src\" --output-type dot | dot -T svg > dependency-graph.svg"
  },
  "dependencies": {
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/reflect-metadata": "^0.0.5",
    "@types/sqlite3": "^3.1.11",
    "autoprefixer": "^10.4.16",
    "date-fns": "^3.0.0",
    "inversify": "^7.5.4",
    "next": "15.3.5",
    "postcss": "^8.4.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^2.12.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.6",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "dependency-cruiser": "^16.10.4",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.4",
    "jest-environment-node": "^30.0.4",
    "jsonc-parser": "^3.3.1",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.3.3"
  }
}
